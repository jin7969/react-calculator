{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "max-depth": ["error", 2],
    "no-param-reassign": "off"
  },
  "plugins": ["prettier"]
}
